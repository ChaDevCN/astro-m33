---
import { fetchData } from "@/lib/fetchDataAll";

import Home from "@/views/home/index.astro";
import Layout from "@/layouts/PageLayout.astro";

const {
  result: {
    data: { menu },
  },
} = await fetchData();

const { keywords, title, description } = menu.find(
  (item) => item.type === "Home"
);
---

<Layout metadata={{ keywords, title, description }}><Home /></Layout>
